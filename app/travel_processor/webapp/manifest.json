{
    "_version": "1.32.0",
    "sap.app": {
        "id": "sap.fe.cap.travel",
        "type": "application",
        "title": "{{title}}",
        "description": "{{description}}",
        "i18n": {
            "bundleName": "sap.fe.cap.travel.i18n.i18n",
            "supportedLocales": [
                "en",
                "de",
                "fr"
            ],
            "fallbackLocale": "en"
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "processor/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "travel-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "travel",
                    "action": "process",
                    "title": "Process Travels",
                    "subTitle": "Process travels",
                    "icon": "sap-icon://flight"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://flight",
            "favIcon": "sap-icon://flight",
            "phone": "sap-icon://flight",
            "phone@2": "sap-icon://flight",
            "tablet": "sap-icon://flight",
            "tablet@2": "sap-icon://flight"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "sap.fe.cap.travel.ext.controller.TravelList"
                    }
                }
            }
        },
        "flexEnabled": true,
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.96.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.fe.cap.travel.i18n.i18n",
                    "supportedLocales": [
                        "en",
                        "de",
                        "fr"
                    ],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "TravelList",
                    "target": [
                        "TravelList"
                    ]
                },
                {
                    "pattern": "/TravelDetail({key}):?query:",
                    "name": "TravelObjectPage",
                    "target": [
                        "TravelList",
                        "TravelObjectPage"
                    ]
                },
                {
                    "pattern": "Travel({key}):?query:",
                    "name": "GuidedTravelCreate",
                    "target": [
                        "TravelList",
                        "GuidedTravelCreate"
                    ]
                },
                {
                    "name": "BookingInvoice",
                    "pattern": "Travel({key})/to_Booking({to_BookingKey}):?query:",
                    "target": [
                        "TravelList",
                        "TravelObjectPage",
                        "BookingInvoice"
                    ]
                }
            ],
            "targets": {
                "TravelList": {
                    "type": "Component",
                    "id": "TravelList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Travel",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                                    "useSemanticDateRange": true
                                },
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "GridTable",
                                        "personalization": {
                                            "sort": true
                                        }
                                    }
                                }
                            },
                            "variantManagement": "Page",
                            "initialLoad": true,
                            "navigation": {
                                "Travel": {
                                    "detail": {
                                        "route": "TravelObjectPage"
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "TravelObjectPage": {
                    "type": "Component",
                    "id": "TravelObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Travel",
                            "navigation": {
                                "to_Booking": {
                                    "detail": {
                                        "route": "BookingInvoice"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "to_Booking/@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "personalization": {
                                            "column": true,
                                            "sort": false
                                        },
                                        "creationMode": {
                                            "name": "Inline",
                                            "createAtEnd": true
                                        }
                                    }
                                }
                            },
                            "editableHeaderContent": false,
                            "content": {
                                "body": {
                                    "sections": {
                                        "FlightInformation": {
                                            "template": "sap.fe.cap.travel.ext.fragment.FlightInformation",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "Travel"
                                            },
                                            "title": "Flight Information",
                                            "type": "XMLFragment"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/Travel({key})"
                },
                "BookingInvoice": {
                    "type": "Component",
                    "id": "BookingInvoice",
                    "name": "sap.fe.core.fpm",
                    "controlAggregation": "endColumnPages",
                    "options": {
                        "settings": {
                            "viewName": "sap.fe.cap.travel.ext.view.Invoice",
                            "entitySet": "Booking"
                        }
                    },
                    "contextPattern": "/Travel({key})/to_Booking({to_BookingKey})"
                },
                "GuidedTravelCreate": {
                    "type": "Component",
                    "id": "GuidedTravelCreate",
                    "name": "sap.fe.core.fpm",
                    "controlAggregation": "midColumnPages",
                    "options": {
                        "settings": {
                            "viewName": "sap.fe.cap.travel.ext.view.TravelCreate",
                            "entitySet": "Travel"
                        }
                    },
                    "contextPattern": "/Travel({key})"
                }
            },
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "sap.fe.cap.sflight"
    }
}
